[host_machine]
system = 'linux'
cpu_family = 'riscv64'
cpu = 'riscv64'
endian = 'little'

[binaries]
c = '/usr/bin/riscv64-linux-gnu-gcc-12'
cpp = '/usr/bin/riscv64-linux-gnu-g++-12'
ar = '/usr/bin/riscv64-linux-gnu-gcc-ar-12'
strip = '/usr/bin/riscv64-linux-gnu-strip'
pkgconfig = '/usr/bin/pkg-config'