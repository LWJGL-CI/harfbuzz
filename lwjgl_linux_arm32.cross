[host_machine]
system = 'linux'
cpu_family = 'arm'
cpu = 'armhf'
endian = 'little'

[binaries]
c = '/usr/bin/arm-linux-gnueabihf-gcc-12'
cpp = '/usr/bin/arm-linux-gnueabihf-g++-12'
ar = '/usr/bin/arm-linux-gnueabihf-gcc-ar-12'
strip = '/usr/bin/arm-linux-gnueabihf-strip'
pkgconfig = '/usr/bin/pkg-config'